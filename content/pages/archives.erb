is_hidden: true
---

<h1>Archives</h1>

<div>
	<% articles_by_year_month.each do |year, month_h| %>
		<% month_h.each do |month, item_a| %>
			<h2><%= year || '' %> <%= to_month_s(month) || '' %></h2>
			<ul>
				<% item_a.each do |item| %>
					<li><%= link_to item[:title], item %></li>
				<% end %>
			</ul>
		<% end %>
	<% end %>
</div>
